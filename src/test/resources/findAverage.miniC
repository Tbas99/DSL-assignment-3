#include <stdio.h>

// Program that finds the average of a number
int main()
{
    int num1;
    int num2;
    double avg;
    double exampleAvg = 6.2563;

    printf("Enter first number: ");
    scanf("%d", num1);
    printf("Enter second number: ");
    scanf("%d", num2);

    avg = (num1+num2)/2;

    //%.2f is used for displaying output upto two decimal places
    printf("Average of %d and %d is: %.2f",num1,num2,avg);

    return 0;
}