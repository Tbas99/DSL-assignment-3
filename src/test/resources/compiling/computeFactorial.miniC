#include<stdio.h>

/*
 * Program that computes a factorial of the input 
 */
int main() {
	int i = 1;
	int fact = 1;
	int number;
	
	printf("Enter a number: ");
	scanf("%d", number);
	
	for(i=1; i<=number; i = i + 1) {
		fact = fact*i;
	}
	
	printf("Factorial of input is: ");
	printf(fact);
	
	return 0;
}